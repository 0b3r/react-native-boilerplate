{
  "name": "RNBoilerplate",
  "version": "0.0.1",
  "scripts": {
    "start": "./node_modules/react-native-cli/index.js start",
    "ios-open": "open /Applications/Xcode.app ios/*.xcodeproj",
    "android": "./node_modules/react-native-cli/index.js run-android",
    "lint": "eslint src test",
    "test": "mocha --recursive --compilers js:babel-core/register -r babel-polyfill -r test/setup",
    "test-watch": "npm run test -- --watch",
    "cover": "babel-node node_modules/.bin/isparta cover _mocha -- --recursive -r test/setup",
    "postinstall": "./postinstall.sh",
    "newproject": "./newproject.sh"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^3.10.1",
    "react-native": "~0.16.0",
    "react-redux": "^3.1.2",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-polyfill": "^6.3.14",
    "babel-preset-rn": "file:babel-preset-rn",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.11.3",
    "fbjs-scripts": "^0.5.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "node-fetch": "^1.3.3",
    "proxyquire": "^1.7.3",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-native-cli": "^0.1.7",
    "redux-logger": "^2.3.1",
    "replace": "^0.3.0",
    "rnpm": "^1.3.4",
    "sinon": "^1.17.2"
  },
  "devEngines": {
    "node": ">= 4.1"
  }
}
